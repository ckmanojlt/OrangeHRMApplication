package com.CoreJava;

public class Pattrens {
	public static void main(String[] args) {
		
	for(int i=1;i<4;i++)	
	{
		System.out.print(i);
	}
System.out.println();
for(int j=4;j<7;j++)	
{
	System.out.print(j);
}
System.out.println();
for(int z=4;z<10;z++)	
{
	System.out.print(z);
}
System.out.println();
	
	
	
	}
	
	
	
	
	
	
	
}